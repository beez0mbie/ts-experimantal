{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": true,
    "jsx": "react",
    "lib": ["ESNext"],
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strict": true,
    "types": ["node", "a-test-clincheck"],
    "outDir": "dist",
    "module": "commonjs",
    "resolveJsonModule": true,
    "target": "ES2020"
  },
  "include": ["src"]
}
